<?xml version="1.0" encoding="UTF-8"?>
<form version="1.0" maxVersion="1.0" type="Form" bindToClass="visao.TelaReajustePreco">
  <properties>
    <property name="layoutManager">
      <object class="com.intellij.uiDesigner.core.AbsoluteLayout" />
    </property>
    <property name="title" type="string" value="Reajuste de PreÃ§os" />
    <property name="defaultCloseOperation" type="int" value="2" /> <!-- JFrame.DISPOSE_ON_CLOSE -->
    <property name="size" type="dimension" width="320" height="160" />
    <property name="locationRelativeTo" type="null" />
  </properties>

  <component id="lblPercentual" class="javax.swing.JLabel" text="Percentual (%):">
