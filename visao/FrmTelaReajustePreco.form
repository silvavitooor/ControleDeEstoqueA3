      <xy x="10" y="20" width="100" height="25" />
    </constraints>
  </component>

  <component id="txtPercentual" class="javax.swing.JTextField">
    <constraints>
      <xy x="120" y="20" width="150" height="25" />
    </constraints>
  </component>

  <component id="btnAplicar" class="javax.swing.JButton" text="Aplicar Reajuste">
    <constraints>
      <xy x="40" y="70" width="120" height="30" />
    </constraints>
  </component>

  <component id="btnLimpar" class="javax.swing.JButton" text="Limpar">
    <constraints>
      <xy x="180" y="70" width="90" height="30" />
    </constraints>
  </component>

</form>

